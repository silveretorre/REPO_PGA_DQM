package test.com.pgamotors.survivorship.physicalPerson 

rule "5_Meta_app_name"
    no-loop true
    dialect "mvel"
    ruleflow-group "5_Meta_app_nameGroup"
when
    $input : RecordIn( dataset.isMostCommon( meta_application_name, "meta_application_name", false) )  
then
    dataset.survive( $input.TALEND_INTERNAL_ID, "meta_application_name" );

end



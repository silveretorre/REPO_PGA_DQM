package test.com.pgamotors.survivorship.physicalPerson 

rule "4_birthDate"
    no-loop true
    dialect "mvel"
    ruleflow-group "4_birthDateGroup"
when
    $input : RecordIn( dataset.isMostCommon( birth_date, "birth_date", true) )  
then
    dataset.survive( $input.TALEND_INTERNAL_ID, "birth_date" );

end



package com.pgamotors.survivorship.physicalPerson 

rule "1_Firstname"
    no-loop true
    dialect "mvel"
    ruleflow-group "1_FirstnameGroup"
when
    $input : RecordIn( dataset.isMostCommon( firstname_norm, "firstname_norm", false) )  
then
    dataset.survive( $input.TALEND_INTERNAL_ID, "lastname_norm" );

end



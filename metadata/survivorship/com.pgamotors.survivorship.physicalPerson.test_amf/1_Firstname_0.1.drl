package com.pgamotors.survivorship.physicalPerson.test_amf 

rule "1_Firstname"
    no-loop true
    dialect "mvel"
    ruleflow-group "1_FirstnameGroup"
when
    $input : RecordIn( dataset.isMostCommon( meta_brand_source, "meta_brand_source", false) )  
then
    dataset.survive( $input.TALEND_INTERNAL_ID, "meta_brand_source" );

end



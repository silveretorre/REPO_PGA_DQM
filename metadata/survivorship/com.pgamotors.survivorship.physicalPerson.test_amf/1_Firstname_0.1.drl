package com.pgamotors.survivorship.physicalPerson.test_amf 

rule "1_Firstname"
    no-loop true
    dialect "mvel"
    ruleflow-group "1_FirstnameGroup"
when
    $input : RecordIn( dataset.isMostCommon( firstname_norm, "firstname_norm", false), meta_application_name.equals("AUTOSPHERE_FR") )  
then
    dataset.survive( $input.TALEND_INTERNAL_ID, "firstname_norm" );

end



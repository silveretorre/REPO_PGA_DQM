package com.pgamotors.survivorship.physicalPerson.test_amf 

rule "4_birthDate"
    no-loop true
    dialect "mvel"
    ruleflow-group "4_birthDateGroup"
when
    $input : RecordIn( dataset.isLatest( birth_date, "birth_date" ) )  
then
    dataset.survive( $input.TALEND_INTERNAL_ID, "birth_date" );

end



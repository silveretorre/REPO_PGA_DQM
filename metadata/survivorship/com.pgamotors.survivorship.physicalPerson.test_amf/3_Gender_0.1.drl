package com.pgamotors.survivorship.physicalPerson.test_amf 

rule "3_Gender"
    no-loop true
    dialect "mvel"
    ruleflow-group "3_GenderGroup"
when
    $input : RecordIn( dataset.isMostCommon( gender_norm, "gender_norm", true), meta_application_name.equals("ICAR") )  
then
    dataset.survive( $input.TALEND_INTERNAL_ID, "gender_norm" );

end



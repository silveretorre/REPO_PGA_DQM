package com.pgamotors.survivorship.physicalPerson.test_amf 

rule "2_Lastname"
    no-loop true
    dialect "mvel"
    ruleflow-group "2_LastnameGroup"
when
    $input : RecordIn( dataset.isMostCommon( lastname_norm, "lastname_norm", false), meta_application_name.equals("SECURYCAR") )  
then
    dataset.survive( $input.TALEND_INTERNAL_ID, "lastname_norm" );

end


